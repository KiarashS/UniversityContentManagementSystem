    @*<div class="container">*@
        <div class="row align-items-center mb-3">
            <div class="col">
                <nav class="topbar-nav is-hoverable shadow-bottom" dir="@(RequestService.IsRtl() ? "rtl" : "ltr")">
                    <ul class="metismenu" id="header-navbar">
                        <partial name="_NavbarPartial" model="@Model" />
                    </ul>
                </nav>
            </div>
        </div>
    @*</div>*@
</div>
